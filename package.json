{
  "name": "examkrackers-components",
  "version": "0.0.10",
  "description": "atom components for examkrackers project",
  "author": "DeSmart",
  "license": "MIT",
  "repository": "https://git.desmart.com/examkrackers/components",
  "homepage": "https://desmart.github.io/components",
  "main": "build/index.js",
  "module": "build/index.modern.js",
  "source": "src/index.tsx",
  "type": "commonjs",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "reset": "rm -rf yarn.lock && touch yarn.lock && yarn install",
    "copy:build-assets": "rm -rf ./build && mkdir -p ./build/assets && cp -R ./src/assets ./build",
    "copy:storybook-assets": "rm -rf ./public && mkdir -p ./public/assets && cp -R ./src/assets ./public",
    "prebuild": "yarn copy:build-assets",
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "prestart": "yarn copy:build-assets",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepublish": "run-s build",
    "prepack": "yarn copy:build-assets && yarn run build",
    "test": "react-scripts test --env=jsdom --watchAll=false",
    "test:build": "yarn-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "prestorybook:start": "yarn copy:storybook-assets",
    "storybook:start": "start-storybook -p 6006 --static-dir public",
    "prestorybook:build": "build-storybook --output-dir public && mkdir -p ./public/assets",
    "storybook:build": "cp -R ./src/assets ./public"
  },
  "peerDependencies": {
    "react": "=16.0.0"
  },
  "dependencies": {
    "@emotion/react": "=11.11.1",
    "@emotion/styled": "=11.11.0",
    "@mui/material": "=5.13.6",
    "@mui/styled-engine-sc": "=5.12.0",
    "@mui/x-date-pickers": "=6.9.0",
    "classnames": "=2.2.6",
    "dayjs": "=1.11.8",
    "depcheck": "=1.4.1",
    "immutability-helper": "=3.1.1",
    "katex": "=0.13.18",
    "lodash.debounce": "=4.0.8",
    "mitt": "=3.0.0",
    "quill": "=1.3.7",
    "ramda": "=0.28.0",
    "react-datepicker": "=4.1.1",
    "react-dnd": "=14.0.5",
    "react-dnd-html5-backend": "=14.1.0",
    "react-lottie": "=1.2.3",
    "react-modal": "=3.12.1",
    "react-player": "=2.9.0",
    "react-select": "=4.2.1",
    "react-tooltip": "=4.2.17",
    "react-useportal": "=1.0.13",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@babel/core": "=7.14.3",
    "@babel/eslint-parser": "=7.14.3",
    "@babel/plugin-transform-regenerator": "=7.18.6",
    "@storybook/addon-actions": "=6.1.21",
    "@storybook/addon-essentials": "=6.1.21",
    "@storybook/addon-links": "=6.1.21",
    "@storybook/cli": "=6.2.9",
    "@storybook/node-logger": "=6.1.21",
    "@storybook/preset-create-react-app": "=3.1.7",
    "@storybook/react": "=6.1.21",
    "@types/jest": "=25.1.4",
    "@types/react": "=17.0.3",
    "@types/react-tooltip": "=4.2.4",
    "@typescript-eslint/eslint-plugin": "=4.4.1",
    "@typescript-eslint/parser": "=2.26.0",
    "babel-loader": "8.1.0",
    "babel-plugin-transform-async-to-promises": "=0.8.18",
    "cross-env": "=7.0.2",
    "eslint": "=7.11.0",
    "eslint-config-prettier": "=6.7.0",
    "eslint-config-standard": "=14.1.0",
    "eslint-config-standard-react": "=9.2.0",
    "eslint-plugin-import": "=2.18.2",
    "eslint-plugin-node": "=11.0.0",
    "eslint-plugin-prettier": "=3.1.1",
    "eslint-plugin-promise": "=4.2.1",
    "eslint-plugin-react": "=7.17.0",
    "eslint-plugin-standard": "=4.0.1",
    "fork-ts-checker-webpack-plugin": "=6.1.1",
    "gh-pages": "=2.2.0",
    "husky": "=7.0.4",
    "jest": "26.6.0",
    "microbundle-crl": "=0.13.8",
    "npm-run-all": "=4.1.5",
