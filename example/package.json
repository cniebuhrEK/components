{
  "name": "components-example",
  "homepage": "https://DeSmart.github.io/components",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "components": "link:..",
    "react": "link:../node_modules/react",
    "react-dom": "link:../node_modules/react-dom",
    "react-scripts": "link:../node_modules/react-scripts"
  },
  "scripts": {
    "start": "rm -rf ./public && mkdir -p ./public/assets && cp -R ../src/assets ./public && start-storybook -p 6006 --static-dir public",
    "build": "build-storybook --output-dir public && mkdir -p ./public/assets && cp -R ../src/assets ./public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/cli": "^6.2.9",
    "@storybook/node-logger": "^6.1.21",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.1.21"
  }
}
